<div class="card">
    <%= link_to image_tag(prototype.image.variant(resize: '250x250')), prototype_path(prototype), class: 'prototype-image' %>
  <div class="card__body">
    <%= link_to prototype.title, root_path, class: :card__title %>
    <p class="card__summary">
       <%= link_to prototype.catch_copy, user_path(prototype.user), class: :card__catch_copy %>
    </p>
    <%= link_to "by #{prototype.user.name}", user_path(prototype.user), class: :card__user %>
  </div>
</div>
